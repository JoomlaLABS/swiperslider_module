{
  "$schema": "https://developer.joomla.org/schemas/json-schema/web_assets.json",
  "name": "mod_joomlalabs_swiperslider_module",
  "version": "2.0.0",
  "description": "Swiper Slider Module Assets",
  "license": "GPL-2.0-or-later",
  "assets": [
    {
      "name": "swiper.bundle",
      "type": "style",
      "uri": "mod_joomlalabs_swiperslider_module/css/swiper-bundle.min.css"
    },
    {
      "name": "swiper.style",
      "type": "style",
      "uri": "mod_joomlalabs_swiperslider_module/css/swiper-style.css",
      "dependencies": ["swiper.bundle"]
    },
    {
      "name": "swiper.pagination-bullet",
      "type": "style",
      "uri": "mod_joomlalabs_swiperslider_module/css/swiper-pagination-bullet.css",
      "dependencies": ["swiper.bundle"]
    },
    {
      "name": "swiper.thumbs",
      "type": "style",
      "uri": "mod_joomlalabs_swiperslider_module/css/swiper-thumbs.css",
      "dependencies": ["swiper.bundle"]
    },
    {
      "name": "swiper.bundle",
      "type": "script",
      "uri": "mod_joomlalabs_swiperslider_module/js/swiper-bundle.min.js",
      "attributes": {
        "defer": true
      }
    },
    {
      "name": "swiper.init",
      "type": "script",
      "uri": "mod_joomlalabs_swiperslider_module/js/swiper-init.js",
      "dependencies": ["swiper.bundle"],
      "attributes": {
        "defer": true
      }
    }
  ]
}
